<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lab1.MemberMapper">

    <select id="getMember" >
        SELECT *
          FROM MEMBER
         WHERE ID = #{ID}
           AND PWD = #{PWD}
    </select>

</mapper>
